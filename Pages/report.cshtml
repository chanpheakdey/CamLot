@page
@model SignalR.Pages.ReportModel
@{
    ViewData["Title"] = "Report page";
}

    <script src="js/jquery.min-3.1.1.js"></script>   
@*<script src="js/jquery.min-2.1.3.js"></script>*@
<script src="js/report.js?v=5"></script>
